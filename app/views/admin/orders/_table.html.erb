    <table class="table">
      <thead>
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>
      </thead>
      <tbody>
        <% orders.each do |order|%>
        <tr>
          <td>
            <%= link_to admin_order_path(order) do %>
              <%= order.created_at.strftime('%Y/%m/%d/%t')%>
            <% end %>
          </td>
          <td><%= order.customer.fist_name%> <%= order.customer.last_name%></td>
          <td><%#= order.order_details.amount%></td>
          <td><%= order.status%></td>
        </tr>
        <% end %>
      </tbody>
    </table>